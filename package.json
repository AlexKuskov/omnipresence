{
  "name": "omnipresence",
  "version": "1.0.0",
  "description": "Google search bar that can be opened almost everywhere",
  "homepage": "",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder"
  },
  "author": {
    "name": "AlexKuskov",
    "email": "a.kuskov98@gmail.com"
  },
  "license": "ISC",
  "build": {
    "appId": "appid",
    "dmg": {
      "contents": [
        {
          "x": 110,
          "y": 150
        },
        {
          "x": 240,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "linux": {
      "category": "app.category.type",
      "target": [
        "AppImage",
        "deb"
      ]
    },
    "win": {
      "target": "squirrel",
      "icon": "build/icon.ico"
    }
  },
  "devDependencies": {
    "electron": "^8.5.2",
    "electron-builder": "^22.3.2"
  },
  "dependencies": {}
}
